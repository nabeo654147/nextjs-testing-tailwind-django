    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "babel-jest": "^27.4.6",
    "eslint": "8.7.0",
